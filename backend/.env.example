HACIENDA_API_URL=https://api.hacienda.go.cr
HACIENDA_CERTIFICATE_PATH=
HACIENDA_CERTIFICATE_PASSWORD=

# Sentry (optional for error tracking)
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0

VITE_APP_NAME="${APP_NAME}"
